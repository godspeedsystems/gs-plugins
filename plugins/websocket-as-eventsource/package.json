{
    "name": "rag-node",
    "version": "1.0.5",
    "description": "Godspeed RAG agent",
    "license": "ISC",
    "author": "Godspeed systems",
    "type": "commonjs",
    "main": "dist/index.js",
    "scripts": {
      "dev": "nodemon --watch src -e ts,js -r dotenv ./src/index.ts",
      "build": "rm -rf dist && mkdir -p dist && npx swc src -d dist --strip-leading-paths && rsync -a --exclude '*.ts' ./src/ ./dist/",
