      "clean": "rm -rf dist"
    },
    "dependencies": {
      "@godspeedsystems/core": "^2.5.4",
      "ws": "^8.18.2",
      "zod": "^3.25.56"
    },
    "devDependencies": {
      "@swc/cli": "^0.7.7",
      "@swc/core": "^1.11.24",
      "@types/ws": "^8.18.1",
      "nodemon": "^3.1.10",
      "typescript": "^5.8.3",
      "ts-node": "^10.9.2"
    },
    "packageManager": "pnpm@10.12.1"
  }
  